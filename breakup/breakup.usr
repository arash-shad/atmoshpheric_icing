c-----------------------------------------------------------------------
      subroutine userbc (ix,iy,iz,iside,ieg)
      include 'SIZE'
      include 'NEKUSE'
      include 'TOTAL' 

      integer e
      e = gllel(ieg)

      ux    =  ux_inf
      uy    =  uy_inf
      uz    =  uz_inf
      temp  =  t_inf
     
      if (boundaryID(iside,e).eq.13) then   ! airfoil surface
         temp = t_wall
      endif 


      return
      end
