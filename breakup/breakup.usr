c----------------------------------------------------------------------------------------
c     Particle Injection at inlet
      subroutine my_place_particle(k,y,rprop)
      include 'SIZE'
      include 'TOTAL'
      include 'PPICLF'

      real*8 y(PPICLF_LRS    , PPICLF_LPART) ! Normal ordering
      real*8 rprop(PPICLF_LRP, PPICLF_LPART) ! Normal ordering

      real*8 rlz,rrz
      common /domainsize/ rlz,rrz

      real*8 rmu,rhof,rg
      common /parameters/ rmu,rhof,rg

      integer*4 ndiam
      parameter(ndiam=8)
      real*8 rhop,dp(ndiam),mup,sigma
      common /dpinfo/ dp,rhop,mup,sigma

      ! Langevin
      real*8 lan_c0,lan_cy,lan_cs,lan_S,lan_cg
      real*8 lan_gij,lan_cw,lan_e,f_w
      common /langevin/ lan_c0,lan_cy,lan_cs,lan_S,
     >              lan_cg,lan_gij,lan_cw,lan_e,f_w
      
      integer i,j,k,k2

      t_scale = lan_cg**(-1)
      v_scale = sqrt(lan_c0*lan_e*t_scale/2)

      k=0
      k2 = 0
      dp_sld  = 40.0d-6

      if (nid.eq.0) then

        do j=1,ndiam       
         do i=1,npart_inj
           k= k+1

           uid = 0.5*(-1  + nid-2)*(-1 +  nid-1) - 1.0
           uid = 0.5*(uid +   k-2)*(uid + k-1) + uid
           uid = 0.5*(uid +   k-2)*(uid + k-1) + uid

           rprop(PPICLF_R_JUID   ,k)  = uid           
           rprop(PPICLF_R_JRHOP  ,k)  = rhop
           rprop(PPICLF_R_JR0TAB ,k)  = dp(j)/2
           rprop(PPICLF_R_JVOLP  ,k)  = pi*dp(j)**3/6.0d0
           rprop(PPICLF_R_JSPL   ,k)  = 1.0
           rprop(PPICLF_R_JPARENT,k)  = k

           if (dp(j) .lt. dp_sld) then
           y(PPICLF_JX,k)  = -0.30 + 0.03*ran2(2)
           y(PPICLF_JY,k)  =  0.08 - 0.05*ran2(2)
           y(PPICLF_JZ,k)  =  rlz + (rrz-rlz)*ran2(2)
           else
           y(PPICLF_JX,k)  = -0.30 + 0.03*ran2(2)
           y(PPICLF_JY,k)  =  0.10 - 0.08*ran2(2)
           y(PPICLF_JZ,k)  =  rlz  + (rrz-rlz)*ran2(2)
           endif

           y(PPICLF_JVX,k) = ux_inf + v_scale*(ran2(2)-0.5)
           y(PPICLF_JVY,k) = uy_inf + v_scale*(ran2(2)-0.5)
           y(PPICLF_JVZ,k) = uz_inf + v_scale*(ran2(2)-0.5) 


         enddo
        enddo
      endif
       
      return
      end

c-----------------------------------------------------------------------

      FUNCTION ran2(idum)
      INTEGER idum,IM1,IM2,IMM1,IA1,IA2,IQ1,IQ2,IR1,IR2,NTAB,NDIV
      REAL ran2,AM,EPS,RNMX
      PARAMETER (IM1=2147483563,IM2=2147483399,AM=1./IM1,IMM1=IM1-1,
     $        IA1=40014,IA2=40692,IQ1=53668,IQ2=52774,IR1=12211,
     $        IR2=3791,NTAB=32,NDIV=1+IMM1/NTAB,EPS=1.2e-7,RNMX=1.-EPS)
c Long period (> 2 ! 1018 ) random number generator of LÃ¢â‚¬â„¢Ecuyer with
c Bays-Durham shuffle and added safeguards. Returns a uniform random deviate
c between 0.0 and 1.0 (exclusive of the endpoint values).
c Call with idum a negative integer to initialize; thereafter, do not alter
c idum between successive deviates in a sequence. RNMX should approximate the
c largest floating value that is less than 1.
      INTEGER idum2,j,k,iv(NTAB),iy
      SAVE iv,iy,idum2
      DATA idum2/123456789/, iv/NTAB*0/, iy/0/
      if (idum.le.0) then
         idum1=max(-idum,1)
         idum2=idum1
         do j=NTAB+8,1,-1
            k=idum1/IQ1
            idum1=IA1*(idum1-k*IQ1)-k*IR1
            if (idum1.lt.0) idum1=idum1+IM1
            if (j.le.NTAB) iv(j)=idum1
         enddo
         iy=iv(1)
      endif
      k=idum1/IQ1
      idum1=IA1*(idum1-k*IQ1)-k*IR1
      if (idum1.lt.0) idum1=idum1+IM1
      k=idum2/IQ2
      idum2=IA2*(idum2-k*IQ2)-k*IR2
      if (idum2.lt.0) idum2=idum2+IM2
      j=1+iy/NDIV
      iy=iv(j)-idum2
      iv(j)=idum1
      if(iy.lt.1)iy=iy+IMM1
      ran2=min(AM*iy,RNMX)
      return
      END
